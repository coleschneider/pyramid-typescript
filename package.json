{
  "name": "pyramid-typescript",
  "version": "1.0.0",
  "description": "A simple configuration for a web app using pyramid and waitress on the backend and typescript with react on the frontend",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js",
    "build": "rimraf dist && NODE_ENV=production webpack --config config/webpack.config.js",
    "lint": "eslint --fix \"client/**/*.+(ts|tsx|js|jsx)\"",
    "format": "prettier --write \"client/**/*.+(ts|tsx|js|jsx)\""
  },
  "keywords": [
    "python",
    "typescript",
    "pyramid",
    "react",
    "webpack"
  ],
  "author": "Cole Schneider",
  "license": "ISC",
  "repository": "https://github.com/coleschneider/pyramid-typescript",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  }
}
